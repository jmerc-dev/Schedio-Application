<Window x:Class="Schedio_Application.MVVM.View.Windows.SectionAddForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Schedio_Application.MVVM.View.Windows"
        mc:Ignorable="d"
        Title="SectionAddForm" Height="450" Width="800"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        AllowsTransparency="True"
        Background="Transparent"
        ResizeMode="NoResize">

    <Border Background="{StaticResource PrimaryBackground}" 
            BorderThickness="1" 
            CornerRadius="10" 
            BorderBrush="{StaticResource SecondaryShadeColor}">
        
    </Border>

</Window>
