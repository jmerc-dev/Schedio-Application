    <SolidColorBrush x:Key="PrimaryBackground" Color="AliceBlue"/>

    <SolidColorBrush x:Key="PrimaryColor" Color="#E2E2E2"/>
    <SolidColorBrush x:Key="SecondaryColor" Color="#265CC5"/>
    <SolidColorBrush x:Key="TertiaryColor" Color="#75A0F5"/>

    <SolidColorBrush x:Key="ShadedSecondaryColor" Color="#2252B0"/>
    <SolidColorBrush x:Key="ShadedTertiaryColor" Color="#5C87DD"/>

    <SolidColorBrush x:Key="PrimaryTextColor" Color="#000000"/>

    <SolidColorBrush x:Key="PrimaryShadeColor" Color="#D2D2D2"/>
    <SolidColorBrush x:Key="SecondaryShadeColor" Color="#808080"/>
    <SolidColorBrush x:Key="TertiaryShadeColor" Color="#737373"/>

    <SolidColorBrush x:Key="RedColor" Color="#C33D3D"/>
